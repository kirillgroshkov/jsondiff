{
  "name": "jsondiff",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "precommit": "lint-staged",
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "clean-www": "rm -rf ./www",
    "upgrade-shared": "yarn upgrade kg-shared @naturalcycles/shared-dev",
    "tslint-all": "tslint './src/**/*.{ts,tsx}' -e './src/components.d.ts' -t stylish --fix",
    "prettier-do": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,scss,graphql,json}'",
    "prettier-all": "yarn prettier-do && yarn tslint-all"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "tslint -e '**/components.d.ts' -t stylish --fix",
      "git add"
    ],
    "src/**/*.{css,scss,graphql,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@stencil/core": "^0.7.24",
    "@stencil/router": "^0.0.28",
    "@stencil/sass": "^0.0.3",
    "jsondiffpatch": "^0.3.9",
    "kg-shared": "^1.0.2",
    "rxjs": "^5.5.6"
  },
  "devDependencies": {
    "@naturalcycles/shared-dev": "^1.0.10",
    "@stencil/dev-server": "^0.0.18",
    "@stencil/utils": "latest",
    "@types/got": "^7.1.7",
    "typescript": "~2.8.1"
  },
  "author": "Kirill Groshkov <1@inventix.ru>",
  "license": "MIT"
}
